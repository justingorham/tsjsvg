{
  "name": "@justingorham/tsjsvg",
  "description": "Typescript json schema and validator generator",
  "version": "2.1.0",
  "author": "Justin Gorham @justingorham",
  "bin": {
    "tsjsvg": "./bin/run"
  },
  "bugs": "https://github.com/justingorham/tsjsvg/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.3",
    "shelljs": "^0.8.3",
    "ts-json-schema-generator": "^0.62.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.7.7",
    "@types/shelljs": "^0.8.6",
    "ajv": "^6.12.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^11.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/justingorham/tsjsvg",
  "keywords": [
    "cli",
    "typescript",
    "json",
    "schema",
    "validator",
    "generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tsjsvg",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "justingorham/tsjsvg",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme && copyfiles -f src/lib/generated-file.ts.hbs lib/lib",
    "test": "node bin/run generate -f tests/generated/validator.ts -p tests/models/**/*.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
